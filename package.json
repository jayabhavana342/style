{
  "name": "styledot",
  "version": "0.0.1",
  "description": "23andMe styleguide",
  "main": "lib/index.js",
  "scripts": {
    "start": "concurrently \"npm run lib:watch\" \"npm run docs:watch\"",
    "lib": "babel src/lib -d lib --copy-files",
    "docs": "webpack --mode production",
    "lib:watch": "babel src/lib --watch --out-dir lib --copy-files --source-maps inline",
    "docs:watch": "webpack-dev-server --mode development -d",
    "test": "jest --testPathIgnorePatterns src --silent"
  },
  "keywords": [],
  "license": "MIT",
  "peerDependencies": {
    "react": "^16.3.0",
    "react-dom": "^16.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-rc.1",
    "@babel/core": "^7.0.0-rc.1",
    "@babel/plugin-proposal-class-properties": "^7.0.0-rc.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-rc.1",
    "@babel/preset-env": "^7.0.0-rc.1",
    "@babel/preset-react": "^7.0.0-rc.1",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^8.2.6",
    "babel-jest": "^23.4.2",
    "babel-loader": "^8.0.0-beta.4",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "concurrently": "^3.5.1",
    "css-loader": "^1.0.0",
    "enzyme": "^3.4.1",
    "enzyme-adapter-react-16": "^1.2.0",
    "eslint": "^5.3.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-jest": "^21.21.0",
    "eslint-plugin-react": "^7.11.1",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.5.0",
    "jquery": "^3.3.1",
    "node-sass": "^4.9.3",
    "react": "^16.3.2",
    "react-anchor-link-smooth-scroll": "^1.0.11",
    "react-dom": "^16.3.2",
    "react-test-renderer": "^16.4.2",
    "react-waypoint": "^8.0.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.22.1",
    "webpack": "^4.6.0",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.3",
    "react-scroll": "^1.7.10",
    "react-syntax-highlighter": "^8.0.1"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "./setupTests.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/23andMe/Style.git"
  }
}
