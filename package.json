{
  "name": "ttam-style",
  "version": "1.0.0",
  "description": "23andMe styleguide",
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-inline-environment-variables": "^0.4.3",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "concurrently": "^3.6.0",
    "jest": "^23.1.0",
    "node-sass": "^4.9.0",
    "nodemon": "^1.17.5",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "rollup": "^0.61.0",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-multi-entry": "^2.0.2",
    "rollup-plugin-node-globals": "^1.2.1",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0"
  },
  "scripts": {
    "test": "jest __test__",
    "start": "concurrently \"npm:build:*\" \"npm:serve\"",
    "serve": "python -m SimpleHTTPServer 8888",
    "watch": "nodemon --ext js,scss,html",
    "ci": "concurrently \"npm:build:*\"",
    "build:site-sass": "node-sass src/styleguide/assets/scss/main.scss site/css/main.css",
    "build:component-sass": "node-sass src/components/main.scss dist/ttam-style.css",
    "build:js": "rollup -c rollup.config.js",
    "build:test-js": "rollup -c rollup.test.js"
  },
  "nodemonConfig": {
    "ignore": [
      "dist/*",
      "site/*",
      "index.html",
      "__test__/*"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/23andMe/Style.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/23andMe/Style/issues"
  },
  "homepage": "https://github.com/23andMe/Style#readme"
}
